<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="269.0" prefWidth="293.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PlaygroundController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Users">
            <items>
                  <MenuItem fx:id="userLoginItem" mnemonicParsing="false" onAction="#userLogin" text="User login" />
              <MenuItem fx:id="createNewUserItem" mnemonicParsing="false" onAction="#createNewUser" text="Create new user" />
                  <MenuItem fx:id="verifyUserItem" mnemonicParsing="false" onAction="#verifyUser" text="Verify user" />
                  <MenuItem fx:id="updateUserItem" mnemonicParsing="false" onAction="#updateUser" text="Update user" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Elements">
            <items>
              <MenuItem fx:id="addNewElementItem" mnemonicParsing="false" onAction="#addNewElement" text="Add New Element" />
                  <MenuItem fx:id="updateElementItem" mnemonicParsing="false" onAction="#updateElement" text="Update Element" />
                  <MenuItem fx:id="getElementsItem" mnemonicParsing="false" onAction="#getElement" text="Get Element" />
                  <MenuItem fx:id="getAllElementItem" mnemonicParsing="false" onAction="#getAllElement" text="Get All Element" />
                  <MenuItem fx:id="getAllNearElementItem" mnemonicParsing="false" onAction="#getAllNearElement" text="Get All Near Elements" />
                  <MenuItem fx:id="getAllElementWithAttributeItem" mnemonicParsing="false" onAction="#getAllElementWithAttribute" text="Get All Element With Attributes" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Activity">
            <items>
              <MenuItem fx:id="createNewActivityItem" mnemonicParsing="false" onAction="#createNewActivity" text="Create new activity" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <SplitPane dividerPositions="0.5" prefHeight="160.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <items>
          <AnchorPane fx:id="leftPane" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" />
          <AnchorPane fx:id="rightPane" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" />
        </items>
      </SplitPane>
   </center>
</BorderPane>
